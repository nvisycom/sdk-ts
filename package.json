{
  "name": "@nvisy/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Nvisy document redaction platform",
  "keywords": [
    "nvisy",
    "document",
    "redaction",
    "sdk",
    "api",
    "typescript",
    "client"
  ],
  "homepage": "https://github.com/nvisycom/sdk#readme",
  "bugs": {
    "url": "https://github.com/nvisycom/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvisycom/sdk.git"
  },
  "license": "MIT",
  "author": "Nvisy <support@nvisy.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --apply src/",
    "format": "biome format --write src/",
    "format:check": "biome format src/",
    "check": "biome check src/",
    "check:fix": "biome check --apply src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "release": "npm run prepublishOnly && npm publish"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/node": "^20.9.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
