{
	"name": "@nvisy/sdk",
	"version": "0.3.0",
	"description": "Official TypeScript SDK for Nvisy document processing platform",
	"type": "module",
	"private": false,
	"keywords": [
		"nvisy",
		"document",
		"privacy",
		"sdk",
		"api",
		"typescript",
		"client"
	],
	"author": "Nvisy <support@nvisy.com>",
	"license": "MIT",
	"homepage": "https://github.com/nvisycom/sdk-ts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nvisycom/sdk-ts.git"
	},
	"bugs": {
		"url": "https://github.com/nvisycom/sdk-ts/issues"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js"
		},
		"./services": {
			"types": "./dist/services/index.d.ts",
			"import": "./dist/services/index.js"
		},
		"./datatypes": {
			"types": "./dist/datatypes/index.d.ts",
			"import": "./dist/datatypes/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE.txt",
		"CHANGELOG.md",
		"CONTRIBUTING.md"
	],
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf dist",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"release": "npm run prepublishOnly && npm publish",
		"generate:check": "curl -sf https://api.nvisy.com/openapi.json -o /dev/null",
		"generate:check:local": "curl -sf http://127.0.0.1:8080/api/openapi.json -o /dev/null",
		"generate": "openapi-typescript https://api.nvisy.com/openapi.json -o ./src/schema/api.d.ts",
		"generate:local": "openapi-typescript http://127.0.0.1:8080/api/openapi.json -o ./src/schema/api.d.ts",
		"generate:clean": "rimraf ./src/schema/api.d.ts"
	},
	"dependencies": {
		"openapi-fetch": "^0.15.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/node": "^25.0.6",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"openapi-typescript": "^7.10.1",
		"rimraf": "^6.1.2",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
